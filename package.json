{
  "name": "tink-connect-example",
  "version": "1.0.0",
  "description": "An example app showing how to use the Tink API in a third-party app.",
  "repository": "github:bjornCarell/tink-connect-example",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node -r esm server/server.js",
    "dev": "concurrently --kill-others-on-fail \"nodemon -r esm server/server.js\" \"npm run client\"",
    "postinstall": "cd client && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "concurrently": "5.1.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "4.17.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "nodemon": "2.0.2"
  }
}
